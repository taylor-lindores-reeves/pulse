	"author": "Taylor Lindores-Reeves",
	"license": "MIT",
	"description": "An example project using Pulse and the pulse-railway-template on railway",
	"scripts": {
		"dev": "concurrently \"next dev\" \"npx tsx prisma/subscriptions/index.ts\"",
		"start": "next start",
		"lint": "next lint",
		"build": "prisma generate && prisma migrate deploy && next build",
		"clean": "rm -rf package-lock.json yarn-lock.json node_modules .cache .next",
		"format": "prettier --config ./prettier.config.js --write 'app/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"@prisma/client": "^5.4.2",
		"@prisma/extension-pulse": "^0.1.6",
		"@radix-ui/react-slot": "^1.0.2",
		"@t3-oss/env-core": "^0.7.0",
		"@t3-oss/env-nextjs": "^0.7.0",
		"@tanstack/react-query": "^4.36.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"dotenv": "^16.3.1",
		"lucide-react": "^0.287.0",
		"next": "^13.5.4",
		"react": "experimental",
		"react-dom": "experimental",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"@types/react": "^18.2.28",
