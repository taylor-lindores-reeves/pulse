		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"prisma": "^5.4.2",
		"tailwind-merge": "^1.14.0",
		"tailwindcss": "^3.3.3",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
